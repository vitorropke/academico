Algoritmo "semnome"

Var
   x, y, result: inteiro

Funcao soma(): inteiro
Var
   z: inteiro
Inicio
   z <- x + y
   retorne z
Fimfuncao

Inicio
   escreva ("Entre com o primeiro valor: ")
   leia (x)
   escreva ("Entre com o segundo valor: ")
   leia (y)
   result <- soma
   escreva ("A soma dos números é: ", result)

Fimalgoritmo